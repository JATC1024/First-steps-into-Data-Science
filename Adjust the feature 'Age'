        "combined_data['Cabin'].fillna('M', inplace = True)\n",
        "# Replaces the cabin by it's first letter.\n",
        "combined_data['Cabin'] = combined_data['Cabin'].apply(lambda x: x[0])\n",
        "sns.countplot(x = 'Cabin', hue = 'Survived', data = combined_data)"
